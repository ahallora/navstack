{
  "name": "navstack",
  "description": "Manages multiple screens with mobile-friendly transitions",
  "keywords": [
    "uinavigationcontroller",
    "navigation",
    "stack"
  ],
  "author": "Rico Sta. Cruz <hi@ricostacruz.com>",
  "version": "0.1.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/rstacruz/navstack.git"
  },
  "main": "navstack",
  "scripts": {
    "test": "mocha",
    "todo": "mocha test/todo.coffee",
    "autotest": "env fast=1 mocha -R progress --watch",
    "coverage": "(cp navstack.js navstack.js~; jscoverage navstack.js; mv navstack-cov.js navstack.js; env cov=1 mocha -R html-cov > coverage.html); mv navstack.js~ navstack.js",
    "prepublish": "make",
    "examples": "serve examples"
  },
  "devDependencies": {
    "mocha": "^1.17.1",
    "chai": "^1.9.0",
    "coffee-script": "^1.7.1",
    "sinon": "^1.8.2",
    "chai-jquery": "^1.2.1",
    "jsdom": "^0.10.1",
    "stylus": "^0.42.3",
    "autoprefixer": "^1.1.20140302",
    "jscoverage": "^0.3.8",
    "serve": "^1.4.0"
  },
  "license": "MIT"
}
